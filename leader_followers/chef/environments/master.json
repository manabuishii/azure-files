{
  "gengine": {
    "role": "master",
    "host_groups": {
      "default": {
        "nodes": []
      }
    },
    "queues": {
      "default": {
        "hostlist": "@default"
      }
    }
  },
  "docker_install":{
    "docker": {
      "distribution": "ubuntu-trusty",
      "version": "1.12.3",
      "members": "azureuser",
      "images": [{"name":"hello-world","tag":"latest"},{"name":"python","tag":"2.7.13"}]
    }
  },
  "run_list": [
    "recipe[gengine]", "recipe[docker-install::docker-install]"
  ]

}
